{
  "name": "tennis-app-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:component": "ng test --component",
    "lint": "ng lint",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "e2e:headless": "cypress run --headless",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:firefox": "cypress run --browser firefox",
    "test:a11y": "cypress run --spec 'cypress/e2e/**/*.a11y.cy.ts'",
    "test:visual": "percy exec -- cypress run",
    "test:all": "npm run lint && npm run test && npm run e2e:headless",
    "serve:test": "ng serve --configuration test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.1",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.2.0",
    "@angular/compiler-cli": "^20.2.0",
    "@cypress/webpack-preprocessor": "^7.0.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/cypress": "^10.1.0",
    "@types/cypress": "^0.1.6",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.2.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "cypress": "^14.5.4",
    "cypress-axe": "^1.7.0",
    "cypress-real-events": "^1.14.0",
    "eslint": "^9.33.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.40.0",
    "wait-on": "^8.0.4"
  }
}
